type T = {
    a: int 0 to 10,
    b: int 0 to 10
};

func add_tuple(t: T): int 0 to 10 {
    let c: int 0 to 20 = t.a + t.b;
    if c < 11 {
        c
    } else {
        c - 10
    }
}

func tuple_add(t: T): { a: int 1 to 11, b: int 1 to 11 } {
    struct { a = t.a + 1, b = t.b + 1 };
}

func main(): int 0 to 1 {
    let t: T = struct { a = 10, b = 10 };
    tuple_add(t);
}